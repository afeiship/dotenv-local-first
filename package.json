{
  "name": "@jswork/dotenv-local-first",
  "version": "1.0.1",
  "description": "Dotenv local first cli.",
  "type": "module",
  "files": [
    "bin"
  ],
  "bin": {
    "envlf": "./bin/index.js",
    "dotenv-local-first": "./bin/index.js"
  },
  "scripts": {
    "dev": "node ./bin/index.js",
    "dev:beta": "NODE_ENV=beta node ./bin/index.js",
    "dev:prod": "NODE_ENV=production node ./bin/index.js",
    "release": "release-it",
    "prepare": "husky",
    "pretty": "prettier --write bin"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run pretty"
    ]
  },
  "keywords": [
    "cli",
    "dotenv",
    "local-first"
  ],
  "license": "MIT",
  "devDependencies": {
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^17.2.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
